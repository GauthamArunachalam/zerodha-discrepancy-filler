{
    "manifest_version" : 3,
    "name" : "Zerodha discrepancy filler",
    "description" : "A extension that can take JSON response and fill the discrepancies in Zerodha",
    "version" : "1.0",
    "action" : {
        "default_popup" : "index.html",
        "default_icon" : "icons/fav_icon.png"
    },
    "permissions" : ["tabs", "activeTab", "scripting"],
    "host_permissions" : [
        "https://console.zerodha.com/portfolio/holdings/discrepancy/*"
    ]
}